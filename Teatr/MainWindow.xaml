<Window x:Class="Teatr.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Teatr"
        mc:Ignorable="d"
        Title="TeatrN" Height="450" Width="800" FontFamily="Segoe UI" Icon="res/T2.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"  ></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="10" ></RowDefinition>
        </Grid.RowDefinitions>
        <Border Grid.Row="0" Background="#FF6FBDB7" ></Border>
        <Border Grid.Row="2" Background="#FF6FBDB7" ></Border>
        <Image Grid.Row="0" Source="res/T2.png" HorizontalAlignment="Left" Margin="5" ></Image>
        <TextBlock Text="Театр" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="24" ></TextBlock>
        <Button Name="BackBtn" Content="Назад" HorizontalAlignment="Right" Click="BackBtn_Click" Background="#FF3F93A6" ></Button>

        <Frame Grid.Row="1" Name="MainFrame" NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered" >

        </Frame>

    </Grid>
</Window>
